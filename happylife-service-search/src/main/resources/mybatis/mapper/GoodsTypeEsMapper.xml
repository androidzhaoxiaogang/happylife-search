<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.happylifeplat.service.search.mapper.GoodsTypeEsMapper" >

    <select id="getNameById" resultType="java.lang.String" parameterType="java.lang.String">
        SELECT  NAME  FROM  goods_type where id = #{id,jdbcType=VARCHAR} AND is_disable= 0 limit 0,1
    </select>



</mapper>
